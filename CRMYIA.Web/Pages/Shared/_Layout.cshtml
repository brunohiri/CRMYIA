                    <button type="button" class="btn btn-tool" data-card-widget="collapse">
                        <i class="fas fa-minus"></i>
                    </button>
                    <button type="button" class="btn btn-tool btn-contato" title="Contacts" data-widget="chat-pane-toggle">
                        <i class="fas fa-comments"></i>
                    </button>
                    <button type="button" class="btn btn-tool btn-close" data-card-widget="remove">
                        <i class="fas fa-times"></i>
                    </button>
                </div>
            </div>
            <!-- /.card-header -->
            <div class="card-body">
                <!-- Conversations are loaded here -->
                <div class="direct-chat-messages chat-body" data-bind="foreach: chatMensagem">

                    @*<div class="direct-chat-msg">
                            <div class="direct-chat-infos clearfix">
                                <span class="direct-chat-name float-left">Alexander Pierce</span>
                                <span class="direct-chat-timestamp float-right">23 Jan 2:00 pm</span>
                            </div>

                            <img class="direct-chat-img" src="../dist/img/user1-128x128.jpg" alt="Message User Image">

                            <div class="direct-chat-text">
                                Is this template really for free? That's unbelievable!
                            </div>

                        </div>*@

                    <div class="direct-chat-msg" data-bind="css: $data.eMeuPessoaChat() === true ? 'right' : ''">
                        <div class="direct-chat-infos clearfix">
                            <span class="direct-chat-name" data-bind="css: $data.eMeuPessoaChat() === true ? 'float-right' : 'float-left', text: $data.nomePessoaChat"></span>
                            <span class="direct-chat-timestamp text-capitalize" data-bind="css: $data.eMeuPessoaChat() === true ? 'float-left' : 'float-right', text: $data.dataCadastroPessoaChat"></span>
                        </div>
                        <!-- /.direct-chat-infos -->
                        <img class="direct-chat-img" src="img/fotoCadastro/foto-cadastro.jpeg" data-bind="attr:{src: $data.imagemPessoaChat}" alt="Message User Image">
                        <!-- /.direct-chat-img -->
                        <div class="direct-chat-text" data-bind="text: $data.mensagemPessoaChat">

                        </div>
                        <!-- /.direct-chat-text -->
                    </div>

                </div>
                <!--/.direct-chat-messages-->
                <!-- Contacts are loaded here -->
                <div class=" direct-chat-contacts">
                    <ul id="lista-usuario" class="contacts-list" data-bind="foreach: filteredChatUsers, event: { scroll: barraListaUsuario }">
                        <li class="idusuario-para" data-bind="click: $parent.buscarContato, attr: {'data-idusuario': $data.idUsuario}">
                            <div data-bind="if: $data.mensagem">
                                <input type="hidden" class="nome" data-bind="value: $data.nome" />

                                <a href="javascript:void(0)">
                                    <i class="fa fa-circle" data-bind="css: $data.logado"></i> <span class="text-decoration-none font-weight-bolder text-white" data-bind="text: $data.nome"></span>
                                    <img class="contacts-list-img" data-bind="attr:{src: $data.imagem}" alt="User Avatar">

                                    <div class="contacts-list-info">
                                        @*<span class="contacts-list-name" data-bind="text: $data.nome"></span>*@
                                        <span class="contacts-list-date float-right" data-bind="text: $data.dataMensagem"></span>
                                        <span class="contacts-list-msg" data-bind="text: $data.mensagem"></span>
                                    </div>
                                    <!-- /.contacts-list-info -->
                                </a>
                                @*<span href="javascript:void(0)" data-bind="ifnot: $parent.eNulo()">Null</span>*@
                            </div>
                            <div class="container text-center" data-bind="ifnot: $data.mensagem">
                                <span><img class="img-circle text-center" width="40" height="40" data-bind="attr:{src: $data.imagem}" alt="" /></span><br />
                                <span>Clique e Envie uma Mensagem para </span><br />
                                <span class="text-center" data-bind="text: $data.nome"></span>
                            </div>
                        </li>
                        <!-- End Contact Item -->
