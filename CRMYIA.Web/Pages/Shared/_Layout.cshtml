        #notificacao-mensagem-tremer {
            /* apenas para dar um formato a div */
            /* animacao */
            animation: tremer .2s;
            animation-iteration-count: infinite;
        }

        /* keyframe para ajustar até onde treme */
        @@keyframes tremer {
            0% {
                margin-left: 0;
            }

            25% {
                margin-left: 15px;
            }

            50% {
                margin-left: 0;
            }

            75% {
                margin-left: -15px;
            }

            100% {
                margin-left: 0;
            }
        }

        #status-usuario li ul {
            position: absolute;
            top: 25px;
            left: 0;
            background-color: #fff;
            display: none;
        }

        #status-usuario li:hover ul, #status-usuario li.over ul {
            display: block;
