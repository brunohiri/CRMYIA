        <!-- DIRECT CHAT -->
        <div id="direct-chat" class="card direct-chat direct-chat-primary d-none float-chat">
            <div class="card-header">
                <h3 id="indentificacao-para" class="card-title"></h3>

                @*<input type="text" class="m-1" name="pesquisa-chat" id="pesquisa-chat" value="" placeholder="Pesquisar" style="border-radius: 0.25rem;" />*@

                <div class="card-tools">
